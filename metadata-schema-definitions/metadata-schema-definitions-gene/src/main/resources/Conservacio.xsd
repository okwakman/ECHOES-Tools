<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.gencat.cat/"
        xmlns:gene="http://www.gencat.cat/"
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        elementFormDefault="qualified">

    <xs:import namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns#" schemaLocation="RDF.xsd"/>
    <xs:include schemaLocation="Diccionaris/ConservacioEstatArqueologic.xsd"/>
    <xs:include schemaLocation="Diccionaris/ConservacioEstatArquitectonic.xsd"/>

    <xs:annotation>
        <xs:documentation>
            GENE First Implementation Schema: Contextual elements (vocabulary terms)
        </xs:documentation>
    </xs:annotation>

    <xs:element name="ConservacioType">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>
                    Estat de conservació del bé.
                </xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="gene:SequenceNoticiaConservacioType">
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="SequenceNoticiaConservacioType">
        <xs:complexContent>
            <xs:extension base="rdf:IdentificadorType">
                <xs:sequence>
                    <xs:element name="conservacioEstat" minOccurs="0">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element name="conservacioEstatArqueologic" type="gene:ConservacioEstatArqueologicType"/>
                                <xs:element name="conservacioEstatArquitectonic" type="gene:ConservacioEstatArquitectonicType"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="conservacioComentari" type="rdf:LiteralType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


</xs:schema>