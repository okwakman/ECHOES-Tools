<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.gencat.cat/"
        xmlns:gene="http://www.gencat.cat/"
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        elementFormDefault="qualified">

    <xs:import namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns#" schemaLocation="RDF.xsd"/>
    <xs:include schemaLocation="Diccionaris/OriginalActual.xsd"/>
    <xs:include schemaLocation="Diccionaris/Utilitzacio.xsd"/>

    <xs:element name="UsType" type="gene:Us">
        <xs:annotation>
            <xs:documentation>
                Dades d'ús del bé.
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="Us">
        <xs:complexContent>
            <xs:extension base="rdf:IdentificadorType">
                <xs:sequence>
                    <xs:choice>
                        <xs:element name="originalActualText" type="rdf:LiteralType"/>
                        <xs:element name="tipusOriginalActual" type="gene:OriginalActualType"/>
                        <xs:element name="tipusUtilitzacio" type="gene:UtilitzacioType"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


</xs:schema>